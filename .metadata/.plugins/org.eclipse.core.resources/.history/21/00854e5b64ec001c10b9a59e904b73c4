package practice;

import comment.Zenhan;

public class Chapter6_1 {
	public static void main(String[] args) {
		comment.Zenhan.doWarusa();
		comment.Zenhan.doTogame();
		callDeae();
		showMondokoro();
	}
	
	public static void callDeae() {
		System.out.println("えぇい、こしゃくな。くせ者だ！であえい！");
	}
	public static void showMondokoro() {
		System.out.println("飛車さん、角さん。もういいでしょう。");
		System.out.println("この紋所が目にはいらぬか！");
		comment.Zenhan.doTogame(); //もう一度、とがめる
	}
}
