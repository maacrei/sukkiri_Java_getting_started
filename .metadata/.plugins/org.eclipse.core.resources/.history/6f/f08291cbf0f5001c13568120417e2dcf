package practice;

import java.util.Scanner;

public class Free {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();
        if(s.equals("Friday")) {
            System.out.println("TGIF");
        } else {
            System.out.println("Still" + " " + s);
        }
	}
}
