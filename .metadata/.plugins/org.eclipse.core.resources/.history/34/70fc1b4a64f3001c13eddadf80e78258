package chapter10;

public class SuperHero extends Hero {
	boolean flying;
	//飛ぶ
	public void fly() {
		this.flying = true;
		System.out.println("飛び上がった！");
	}
	//着地する
	public void land() {
		this.flying = false;
		System.out.println("着地した！");
	}
	//逃げる　親クラスにも定義してあるが、子クラスで再定義(上書き)するメソッド
	public void run() {
		System.out.println(this.name + "は撤退した");
	}
}
